<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:jaxws="http://cxf.apache.org/jaxws"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://cxf.apache.org/jaxws
		http://cxf.apache.org/schemas/jaxws.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        http://www.springframework.org/schema/context
      	http://www.springframework.org/schema/context/spring-context.xsd
        ">

    <dubbo:reference id="connectionInfoService" interface="com.hisign.code.api.database.ConnectionInfoService" timeout="20000" retries="0"/>

    <dubbo:reference id="tableColumnService" interface="com.hisign.code.api.database.TableColumnService" timeout="20000" retries="0"/>

    <dubbo:reference id="templateSqlService" interface="com.hisign.code.api.database.TemplateSqlService" timeout="20000" retries="0"/>

    <dubbo:reference id="developSqlService" interface="com.hisign.code.api.database.DevelopSqlService" timeout="20000" retries="0"/>

    <dubbo:reference id="tableConnectionService" interface="com.hisign.code.api.database.TableConnectionService" timeout="20000" retries="0"/>

    <dubbo:reference id="translationDictService" interface="com.hisign.code.api.database.TranslationDictService" timeout="20000" retries="0"/>

</beans>